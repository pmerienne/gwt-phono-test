<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit 2.3.0//EN" "http://google-web-toolkit.googlecode.com/svn/tags/2.3.0/distro-source/core/src/gwt-module.dtd">
<module rename-to="PhonoTest">
	<!-- Inherit the core Web Toolkit stuff. -->
	<inherits name='com.google.gwt.user.User' />
	<!-- inherit css based theme -->
	<inherits name='com.google.gwt.user.theme.standard.Standard' />

	<!-- Inherit Phono library -->
	<inherits name="com.google.gwt.phono.Phono"/>

	<!-- Specify the paths for translatable code -->
	<source path="client" />

	<!-- Specify the app entry point class. -->
	<entry-point class="com.google.gwt.phono.test.client.PhonoTest" />

	<!-- 1 permutation for dev -->
	<set-property name="user.agent" value="safari,gecko1_8" />
</module>
